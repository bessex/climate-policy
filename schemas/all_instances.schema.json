{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "document_id": {
                "type": "integer"
            },
            "instance_id": {
                "type": "string",
                "pattern": "^\\d+_\\d+$"
            },
            "match": {
                "type": "string"
            },
            "start": {
                "type": "integer",
                "minimum": 0
            },
            "end": {
                "type": "integer",
                "minimum": 0
            }
        },
        "required": ["document_id", "instance_id", "match", "start", "end"]
    }
}
