{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "patternProperties": {
        "^\\d+_\\d+$": {
            "type": "object",
            "properties": {
                "window": {
                    "type": "string"
                },
                "polarity": {
                    "type": "string",
                    "enum": ["positive", "negative"]
                }
            },
            "required": ["window", "polarity"]
        }
    },
    "additionalProperties": false
}
